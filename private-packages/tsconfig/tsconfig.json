{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"noEmit": true,
		"target": "ESNext",
		"module": "ES2020",
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,

		"strictBindCallApply": true,
		"strictFunctionTypes": true,
		"strictNullChecks": true,

		"noImplicitAny": true,
		"noImplicitThis": true
	}
}
